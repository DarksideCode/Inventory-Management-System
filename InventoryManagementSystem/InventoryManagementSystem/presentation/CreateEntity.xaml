<Window x:Class="InventoryManagementSystem.presentation.CreateEntity"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InventoryManagementSystem.presentation"
        mc:Ignorable="d"
        Title="CreateEntity" Height="300" Width="300">
    <Grid>
        <Frame x:Name="CreateFrame" Content="Frame" HorizontalAlignment="Left" Height="269" VerticalAlignment="Top" Width="292"/>
        <Button x:Name="DiskCancel" Content="Abbrechen" HorizontalAlignment="Left" Margin="170,237,0,0" VerticalAlignment="Top" Width="75" Click="DiskCancel_Click">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="#FFD5E8D4" Offset="0"/>
                    <GradientStop Color="#FF97D077" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>
    </Grid>
</Window>
